<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <ImageView
        android:id="@+id/item00"
        style="@style/Item.00"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true" />

    <ImageView
        android:id="@+id/item01"
        style="@style/Item.01"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@id/item00"/>

    <ImageView
        android:id="@+id/item02"
        style="@style/Item.02"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@id/item01"/>

    <ImageView
        android:id="@+id/item10"
        style="@style/Item.10"
        android:layout_below="@id/item00"
        android:layout_alignLeft="@id/item00"/>

    <ImageView
        android:id="@+id/item11"
        style="@style/Item.11"
        android:layout_below="@id/item01"
        android:layout_alignLeft="@id/item01"/>

    <ImageView
        android:id="@+id/item12"
        style="@style/Item.12"
        android:layout_below="@id/item02"
        android:layout_alignLeft="@id/item02"/>

</RelativeLayout>