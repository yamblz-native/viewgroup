<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/constraintLayout">

    <ImageView style="@style/Item.00"
        android:contentDescription="@string/item00"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:id="@+id/imageView2"
        app:layout_constraintLeft_toLeftOf="@+id/constraintLayout"
        app:layout_constraintTop_toTopOf="@+id/constraintLayout" />

    <ImageView style="@style/Item.01"
        android:contentDescription="@string/item01"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:id="@+id/imageView"
        app:layout_constraintLeft_toRightOf="@+id/imageView2"
        app:layout_constraintTop_toTopOf="@+id/imageView2" />

    <ImageView style="@style/Item.02"
        android:contentDescription="@string/item02"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintLeft_toRightOf="@+id/imageView"
        app:layout_constraintTop_toTopOf="@+id/imageView2" />

    <ImageView style="@style/Item.10"
        android:contentDescription="@string/item10"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:id="@+id/imageView3"
        app:layout_constraintLeft_toLeftOf="@+id/constraintLayout"
        app:layout_constraintTop_toBottomOf="@+id/imageView2" />

    <ImageView style="@style/Item.11"
        android:contentDescription="@string/item11"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:id="@+id/imageView4"
        app:layout_constraintLeft_toRightOf="@+id/imageView3"
        app:layout_constraintTop_toTopOf="@+id/imageView3" />

    <ImageView style="@style/Item.12"
        android:contentDescription="@string/item12"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintLeft_toRightOf="@+id/imageView4"
        app:layout_constraintTop_toTopOf="@+id/imageView3" />


</android.support.constraint.ConstraintLayout>