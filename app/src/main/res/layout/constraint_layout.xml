<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    tools:ignore="ContentDescription,RtlHardcoded">

    <ImageView
        android:id="@+id/iv00"
        style="@style/Item.00"
        />

    <ImageView
        android:id="@+id/iv01"
        style="@style/Item.01"
        app:layout_constraintLeft_toRightOf="@id/iv00"/>

    <ImageView
        android:id="@+id/iv02"
        style="@style/Item.02"
        app:layout_constraintLeft_toRightOf="@id/iv01"/>

    <ImageView
        android:id="@+id/iv10"
        style="@style/Item.10"
        app:layout_constraintTop_toBottomOf="@id/iv00"/>

    <ImageView
        android:id="@+id/iv11"
        style="@style/Item.11"
        android:layout_below="@id/iv01"
        app:layout_constraintLeft_toRightOf="@id/iv10"
        app:layout_constraintTop_toBottomOf="@id/iv01"/>

    <ImageView
        android:id="@+id/iv12"
        style="@style/Item.12"
        android:layout_below="@id/iv02"
        app:layout_constraintLeft_toRightOf="@id/iv11"
        app:layout_constraintTop_toBottomOf="@id/iv02"/>

</android.support.constraint.ConstraintLayout>