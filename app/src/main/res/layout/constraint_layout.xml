<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    tools:ignore="ContentDescription,RtlHardcoded"
    android:id="@+id/constraintLayout">


    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/ic_fast_forward"
        app:layout_constraintLeft_toRightOf="@+id/imageView9"
        app:layout_constraintTop_toTopOf="@+id/imageView9"/>

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/ic_fast_rewind"
        app:layout_constraintLeft_toLeftOf="@+id/constraintLayout"
        app:layout_constraintTop_toTopOf="@+id/constraintLayout"/>

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/ic_play_circle_filled"
        app:layout_constraintLeft_toRightOf="@+id/imageView8"
        app:layout_constraintTop_toTopOf="@+id/imageView8"/>

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/ic_equalizer"
        app:layout_constraintLeft_toRightOf="@+id/imageView11"
        app:layout_constraintTop_toBottomOf="@+id/imageView9"/>

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/ic_repeat"
        app:layout_constraintLeft_toLeftOf="@+id/imageView8"
        app:layout_constraintTop_toBottomOf="@+id/imageView8"/>

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/ic_shuffle"
        app:layout_constraintLeft_toRightOf="@+id/imageView10"
        app:layout_constraintTop_toBottomOf="@+id/imageView7"/>

</android.support.constraint.ConstraintLayout>